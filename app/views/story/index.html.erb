<% @story.each do |story| %>
  <p>
      <%= story.title %> 
      <%= story.stage %>
      <%= story.date %>
      <%= link_to '削除', "/story/#{story.id}", data: {turbo_method: :delete} %>
      <%= link_to '表示', "/story/#{story.id}" %>
      <%= link_to '編集', "/story/#{story.id}/edit" %>
  </p>
<% end %>
<p><%= link_to '新規追加', '/story/new' %></p>